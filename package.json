{
  "name": "apiserver",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "https://github.com/somavoyager/apiserver.git",
  "author": "Voyager LLC",
  "private": true,
  "scripts": {
    "watch-debug": "tsc-watch --noClear --onSuccess 'node --inspect=0.0.0.0:9235 --nolazy ./build/index.js' --onFailure 'echo Beep! Compilation Failed'",
    "debug": "NODE_ENV=dev node --inspect=0.0.0.0:9228 ./build/index.js",
    "debug-windows": "set NODE_ENV=dev && tsc-watch.bat",
    "debug-break": "node --nolazy --inspect-brk=9235 ./build/index.js",
    "test": "NODE_ENV=test jest --runInBand ./tests",
    "unit-tests": "NODE_ENV=test DB_USED=sqllite jest --coverage=true --runInBand ./unit-tests",
    "unit-test-specific": "NODE_ENV=test DB_USED=sqllite jest --runInBand ./unit-tests \"--testNamePattern\" ",
    "test-specific": "NODE_ENV=test jest --runInBand ./tests \"--testNamePattern\" ",
    "test-server-watch": "tsc-watch --noClear --onSuccess './debug.sh' --onFailure 'echo Beep! Compilation Failed'",
    "test-server": "NODE_ENV=test DB_USED=sqllite node build/index.js",
    "test-server-windows": "test-server.bat",
    "test-windows": "NODE_ENV=test jest --runInBand",
    "script-tests-old": "node --nolazy -r ts-node/register/transpile-only script-tests/testdriver.ts",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest-disabled": "npm run compile",
    "posttest-disabled": "npm run check",
    "run-pg": "docker-compose -f docker-compose-pg.yaml up",
    "script-tests": "node build/script-tests/testdriver.js ./script-tests/script"
  },
  "dependencies": {
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "apollo-boost": "^0.4.9",
    "apollo-graphql": "^0.5.0",
    "apollo-server-express": "^2.14.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "colorize": "^0.1.0",
    "express": "^4.17.1",
    "graphql": "^14.2.1",
    "gts": "^2.0.2",
    "hashids": "^2.2.1",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "merge-graphql-schemas": "^1.7.8",
    "module-alias": "^2.2.2",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "tsc-watch": "^4.2.8",
    "typeorm": "^0.2.25",
    "uuid": "^8.1.0",
    "winston": "^3.2.1",
    "yarn": "^1.22.4",
    "zipkin": "^0.22.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "copyfiles": "^2.3.0",
    "gts": "^2.0.2",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "sqlite3": "^4.2.0"
  },
  "_moduleAliases": {
    "@src": "./build/src/",
    "@test": "./build/test/",
    "@": "./"
  }
}
