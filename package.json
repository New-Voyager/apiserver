{
  "name": "apiserver",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "https://github.com/somavoyager/apiserver.git",
  "author": "Voyager LLC",
  "private": true,
  "scripts": {
    "watch-debug": "tsc-watch --noClear --onSuccess 'node --inspect=0.0.0.0:9235 --nolazy ./build/index.js' --onFailure 'echo Beep! Compilation Failed'",
    "debug": "NODE_ENV=dev node --inspect=0.0.0.0:9228 ./build/index.js",
    "debug-break": "node --nolazy --inspect-brk=9235 ./build/index.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test-specific": "NODE_ENV=test jest --runInBand \"--testNamePattern\" \"start a new game\"",
    "test-server-watch": "tsc-watch --noClear --onSuccess './debug.sh' --onFailure 'echo Beep! Compilation Failed'",
    "test-server": "NODE_ENV=test DB_USED=sqllite node build/index.js",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest-disabled": "npm run compile",
    "posttest-disabled": "npm run check"
  },
  "dependencies": {
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "apollo-graphql": "^0.4.5",
    "apollo-server-express": "^2.14.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "colorize": "^0.1.0",
    "express": "^4.17.1",
    "graphql": "^14.2.1",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.8",
    "module-alias": "^2.2.2",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "tsc-watch": "^4.2.8",
    "typeorm": "^0.2.25",
    "uuid": "^8.1.0",
    "winston": "^3.2.1",
    "zipkin": "^0.22.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "apollo-boost": "^0.4.9",
    "copyfiles": "^2.3.0",
    "gts": "^2.0.2",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@src": "./build/src/",
    "@test": "./build/test/",
    "@": "./"
  }
}
