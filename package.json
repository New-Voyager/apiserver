{
  "name": "apiserver",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "https://github.com/somavoyager/apiserver.git",
  "author": "Voyager LLC",
  "private": true,
  "scripts": {
    "watch-debug": "npm run copy-files && NOTIFY_GAME_SERVER=1 BOTRUNNER_URL=http://localhost:8081 TIMER_URL=http://localhost:8082 NATS_URL=${NATS_URL:-'nats://localhost:4222'} NATS_ENABLED=1 POSTGRES_HOST=${POSTGRES_HOST:-localhost} POSTGRES_PORT=${POSTGRES_PORT:-5432} POSTGRES_USER=${POSTGRES_USER:-game} POSTGRES_PASSWORD=${POSTGRES_PASSWORD:-game} NODE_ENV=dev tsc-watch --noClear --onSuccess 'node --inspect=0.0.0.0:9235 --nolazy ./build/index.js' --onFailure 'echo Beep! Compilation Failed'",
    "run-docker": "NOTIFY_GAME_SERVER=1 NATS_URL=${NATS_URL:-'nats://nats:4222'} NATS_ENABLED=1 POSTGRES_HOST=${POSTGRES_HOST:-localhost} POSTGRES_PORT=${POSTGRES_PORT:-5432} POSTGRES_USER=${POSTGRES_USER:-game} POSTGRES_PASSWORD=${POSTGRES_PASSWORD:-game} NODE_ENV=dev node --inspect=0.0.0.0:9235 --nolazy ./build/index.js",
    "debug": "npm run copy-files && NODE_ENV=dev POSTGRES_HOST=${POSTGRES_HOST:-localhost} POSTGRES_PORT=${POSTGRES_PORT:-5432} POSTGRES_USER=${POSTGRES_USER:-game} POSTGRES_PASSWORD=${POSTGRES_PASSWORD:-game} node --inspect=0.0.0.0:9228 ./build/index.js",
    "debug-windows": "set NODE_ENV=dev && tsc-watch.bat",
    "debug-break": "node --nolazy --inspect-brk=9235 ./build/index.js",
    "test": "NODE_ENV=test jest --runInBand ./tests",
    "test1": "NODE_ENV=test jest --runInBand ./tests \"--testNamePattern\" \"wait list seating\"",
    "unit-tests": "NODE_ENV=test DB_USED=sqllite npx jest --forceExit --coverage=true --runInBand ./unit-tests",
    "unit-test-specific": "NODE_ENV=unit-test DB_USED=sqllite jest --forceExit --runInBand ./unit-tests \"--testNamePattern\" \"Handtest: Bookmark hands\"",
    "test-specific": "NODE_ENV=test jest --runInBand ./tests \"--testNamePattern\" \"change display name\"",
    "test-server-watch": "NODE_ENV=test NATS_URL=${NATS_URL:-'nats://localhost:4222'} DB_USED=sqllite tsc-watch --noClear --onSuccess 'node --inspect=0.0.0.0:9235 --nolazy ./build/index.js' --onFailure 'echo Beep! Compilation Failed'",
    "test-server": "NODE_ENV=test NATS_URL=${NATS_URL:-'nats://nats:4222'} DB_USED=sqllite node build/index.js",
    "run-server": "node build/index.js",
    "test-server-windows": "test-server.bat",
    "test-windows": "set NODE_ENV=test && jest --runInBand ./tests",
    "script-tests-old": "node --nolazy -r ts-node/register/transpile-only script-tests/testdriver.ts",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc && npm run copy-files",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest-disabled": "npm run compile",
    "posttest-disabled": "npm run check",
    "run-pg": "docker-compose -f docker-compose-pg.yaml up",
    "run-redis": "docker-compose -f docker-compose-redis.yaml up -d",
    "stop-redis": "docker-compose -f docker-compose-redis.yaml down",
    "script-tests": "node build/script-tests/testdriver.js ./script-tests/script",
    "copy-files": "npm run copy-google-svc",
    "copy-google-svc": "mkdir -p build/src/google-services/ && cp src/google-services/* build/src/google-services/"
  },
  "dependencies": {
    "@googleapis/docs": "^0.2.0",
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "agora-access-token": "^2.0.4",
    "apollo-boost": "^0.4.9",
    "apollo-graphql": "^0.5.0",
    "apollo-server-express": "^2.14.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "colorize": "^0.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.6",
    "google-play-billing-validator": "^2.1.3",
    "googleapis": "^80.1.0",
    "graphql": "^14.2.1",
    "gts": "^2.0.2",
    "hashids": "^2.2.1",
    "humanize-duration": "^3.25.0",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "merge-graphql-schemas": "^1.7.8",
    "module-alias": "^2.2.2",
    "nats": "^1.4.12",
    "node-apple-receipt-verify": "^1.9.3",
    "pg": "^8.2.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsc-watch": "^4.2.8",
    "typeorm": "^0.2.25",
    "uuid": "^8.1.0",
    "winston": "^3.2.1",
    "yaml": "^1.10.2",
    "yarn": "^1.22.4",
    "zipkin": "^0.22.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "copyfiles": "^2.3.0",
    "gts": "^2.0.2",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "sqlite3": "^x",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@src": "./build/src/",
    "@test": "./build/test/",
    "@": "./"
  }
}
